    
    public void next(String x, String y){
        if(TimeCom(x,y) == true){
            sendWhatsAppMessageTo();
        }
    }

    public boolean TimeCom(String x, String y){  //Der wundersch√∂ne Zeitvergleich
        if(x.equals(y)) {
            Log.v("INFO", "seems true" + x + y);
            return true;
        }
        else{
            return false;
        }
    }
